<% title 'Upload a CSV file' %>

<%= form_tag upload_indicators_path, :multipart => true do |f| %>
	<%= f.error_messages %>

	<div>
		<%= f.file_field :file, 'CSV File to Upload', :accept => 'text/csv'
	<div>
	<div>
	  <%= f.submit, "Proces CSV File" %>
	<div>
<% end %>

<%= link_to 'Back', indicators_path %>
