<% title 'Listing data' %>

<table>
  <tr>
    <th>Indicator</th>
    <th>Common</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @data.each do |datum| %>
  <tr>
    <td><%= datum.indicator_id %></td>
    <td><%= datum.common_id %></td>
    <td><%= datum.value %></td>
    <td><%= link_to 'Show', datum %></td>
    <td><%= link_to 'Edit', edit_datum_path(datum) %></td>
    <td><%= link_to 'Destroy', datum, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Datum', new_datum_path %>
